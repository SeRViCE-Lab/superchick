      </geometry>
      <origin xyz="0 0 0.1" rpy="0 0 0" />
    </collision>
  </link>

  <joint name="base_link_to_cush_link1" type="continuous">
    <parent link="base_link"/>
    <child link="cush_link1"/>
    <origin xyz="0 0 0" />
  </joint>
  
